PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?graph ?class ?classLabel ?count
WHERE {
    {
        SELECT ?graph ?class (count(?class) AS ?count)  
        WHERE {
            GRAPH ?graph {
                [] a ?class .
            }
        }
        GROUP BY ?graph ?class
        ORDER BY desc(?count)
    }
    
    OPTIONAL {
        ?class rdfs:label ?classLabel .
    }
}
